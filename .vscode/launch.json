  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Django",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/example/manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Python: Django Test",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/example/manage.py",
      "args": ["test"],
      "preLaunchTask": "Build",
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Python: Django Pro",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/example",
      "program": "../env/bin/gunicorn",
      "args": ["project.wsgi:application", "-b", "localhost:9000"],
      "django": true
    }
  ]
}
